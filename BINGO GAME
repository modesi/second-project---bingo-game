#import random
import random

#create a list of bingo words
bingo_words = ["Bingo", "Lucky", "Jackpot", "Winner", "Money", "Fortune", "Prize", "Gambling", "Casino", "Happiness"]

#create a list of bingo numbers
bingo_numbers = []

#generate random numbers between 1 and 10
for i in range(10):
    bingo_numbers.append(random.randint(1,10))

#print bingo words
print("Bingo words:")
for bingo_word in bingo_words:
    print(bingo_word)

#print bingo numbers
print("\nBingo numbers:")
for bingo_number in bingo_numbers:
    print(bingo_number)

#ask the user for a bingo number
user_number = int(input("\nPlease enter a number from 1 to 10: "))

